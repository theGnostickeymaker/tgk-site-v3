{# topbar.njk â€” unified header with dynamic Part meta #}
{% set crumbs = breadcrumbs or [ { title: "The Gnostic Key", url: "/" } ] %}
{% set pglyph = pillarGlyph or pillarGlyphs or "âœ¶" %}
{% set isIndex = page.fileSlug == "index" %}
{% set hasParts = scrollsMeta and (scrollsMeta | length) > 0 %}

<header class="site-header minimal" role="banner">
  <div class="site-brand">
    {% if not hideBreadcrumbs %}
    <nav class="breadcrumbs" aria-label="Breadcrumb">
      {% for c in crumbs %}
        {% if not loop.last %}
          {% if c.url %}
            <a href="{{ c.url }}" class="header-link">{{ c.title }}</a>
          {% else %}
            <span class="header-link">{{ c.title }}</span>
          {% endif %}
          {% if loop.index < loop.length - 1 %}
            <span class="sep"> | </span>
          {% endif %}
        {% endif %}
      {% endfor %}
    </nav>
  {% endif %}

  </div>

  {# Decorative glyphs #}
  <div class="glyphs" aria-hidden="true">
    {% for g in glyphRow or [pglyph, "âœ¶", pglyph] %}
      <span class="glyph-scroll">{{ g }}</span>
    {% endfor %}
  </div>

{# Optional: Series Group above title, e.g., "The Afterlife Series" #}
  {% if seriesGroup %}
    <h2 class="series-title">{{ seriesGroup }}</h2>
  {% endif %}

  <h1 class="main-logo">
    {{ scrollGroup or pillarHeading or seriesGroup or episodeGroup or title or pillarName }}
  </h1>

  {% if pillarTagline or tagline %}
    <p class="main-tagline">{{ pillarTagline or tagline }}</p>
  {% endif %}

  {# Meta line: Series Â· Episode Â· Part title â€” shown for scroll parts and group entries #}
  {% if not isIndex and (episode or hasParts or partTitle) %}
    {% if hasParts %}
      {% set first = scrollsMeta[0] %}
    {% else %}
      {% set first = null %}
    {% endif %}

    <div class="meta-row">
      <div id="scroll-meta" class="scroll-meta" role="status" aria-live="polite">
        <span class="series-label">Series {{ (seriesMeta and seriesMeta.number) or "1" }}</span>

        {% if episode %}
          <span class="dot" aria-hidden="true">Â·</span>
          <span class="episode-label">Episode {{ episode }}</span>
        {% endif %}

        {% if partNumeral or partTitle %}
          <span class="dot" aria-hidden="true">Â·</span>
          <span class="scroll-label">
            Part
            {% if partNumeral %}
              <span id="scroll-meta-numeral">{{ partNumeral }}</span>
            {% endif %}
            {% if partTitle %}
              : <span id="scroll-meta-title">{{ partTitle }}</span>
            {% endif %}
          </span>
        {% elif first %}
          <span class="dot" aria-hidden="true">Â·</span>
          <span class="scroll-label">
            Part <span id="scroll-meta-numeral">{{ first.numeral }}</span>:
            <span id="scroll-meta-title">{{ first.title }}</span>
          </span>
        {% endif %}
      </div>

      <button
