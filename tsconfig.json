{
  "include": [
    "src/**/*.js",
    "eleventy.config.js",
    "scripts/**/*.js"
  ],
  "exclude": [
    "node_modules",
    "_site"
  ],
  "compilerOptions": {
    "target": "ES2022",
    "module": "CommonJS",
    "moduleResolution": "Node",
    "allowJs": true,
    "checkJs": false,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "noEmit": true
  }
}
